{
  "expo": {
    "name": "Kayhan Audio",
    "slug": "my-expo-app",
    "version": "1.0.0",
    "extra": {
      "API_URL": "https://api.kayhanaudio.com.au",
      "eas": {
        "projectId": "81a09624-13cf-46da-a87e-bfd9f12fe44a"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-secure-store"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.karandhiman.myexpoapp"
    },
    "owner": "karandhiman"
  }
}
